{
  "extends": ["react-app", "eslint:recommended"],
  "ignorePatterns": ["node_modules"],
  "rules": {
    "no-use-before-define": ["error", { "functions": true, "classes": true }],
    "no-unused-vars": [
      "error",
      {
        "vars": "all",
        "varsIgnorePattern": "^_",
        "argsIgnorePattern": "^_",
        "args": "all",
        "ignoreRestSiblings": false
      }
    ]
  }
}
